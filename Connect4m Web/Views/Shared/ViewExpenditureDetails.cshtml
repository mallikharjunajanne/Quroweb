x => x.MonthId,
 new SelectList(ViewBag.Months, "Value", "Text"),
 " ", new { @class = "form-control readonly", @id = "dropdown_Month_SEM", @style = "color:black;", @disabled = true })

                                                </div>
                                                <span asp-validation-for="MonthId" class="text-danger"></span>

                                            </div>


                                            <div class="form-group col-2 ">
