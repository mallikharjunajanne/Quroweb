@*@model Connect4m_Web.Models.Attendenceproperites.UserScreen.AdmissionProcess*@
@model UserScreen.AdmissionProcess
@{
    //Layout = "~/Views/Shared/_Layout.cshtml";
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>


</head>
<body>
    <div class="text-right">      
        <span data-value="@ViewBag.Returnmessage" id="Insert_Updatespanid" style="display:block;"></span>
        <a href="" id="Backtosearchlnk">Back to Search</a>
    </div>
    <form id="NewAdmissionform" enctype="multipart/form-data">
        <div class="row mb-2">
            <div class="col-md-12 col-lg-12 mb" id="Studentdetails">
                <div class="card mb-4">
                    <div class="card-header d-flex align-items-center justify-content-between">
                        <h5 class="mb-0">STUDENT DETAILS</h5> <small class="text-muted float-end">* INDICATES MANDATORY FIELD</small>
                    </div>
                    <div class="card-body">
                        <div class="row">
                            <div class="col">
                                <div class="row mb-3">
                                    <div class="mb row">
                                        <label class="col-sm-4 col-form-label text-end"></label>
                                        <div class="col-sm-8">
                                            <label class="col-sm-3 col-form-label text-center">First Name</label>
                                            <label class="col-sm-3 col-form-label text-center">Middle Name</label>
                                            <label class="col-sm-2 col-form-label text-center">Last Name</label>
                                        </div>
                                    </div>
                                    <div class="mb-3 row">
                                        <label class="col-sm-4 col-form-label space required text-end">Name of the child</label>
                                        <div class="col-sm-2">
                                            <input type="text" asp-for="FirstName" class="col-sm-3 form-control" id="txtFirstName" placeholder="FirstName" autocomplete="off" />
                                            <span asp-validation-for="FirstName" class="text-danger"></span>
                                            <input type="hidden" asp-for="RegistrationUserId" class="col-sm-3 form-control" id="txtRegistrationUserId"/>
                                        </div>
                                        <div class="col-sm-2">
                                            <input type="text" asp-for="MiddleName" class="col-sm-3 form-control" id="txtMiddleName" placeholder="MiddleName" autocomplete="off" />
                                        </div>
                                        <div class="col-sm-2">
                                            <input type="text" asp-for="LastName" class="col-sm-3 form-control" id="txtLastName" placeholder="LastName" autocomplete="off" />
                                            <span asp-validation-for="LastName" class="text-danger"></span>
                                        </div>
                                    </div>
                                    <div class="mb-3 row">
                                        <label class="col-sm-4 col-form-label space required text-end">Date of Birth</label>
                                        <div class="col-sm-4">
                                            <input type="date" asp-for="DOB" class="form-control" id="txtDOB" autocomplete="off" />
                                            <span asp-validation-for="DOB" class="text-danger"></span>
                                        </div>
                                    </div>
                                    <div class="mb-3 row">
                                        <label class="col-sm-4 col-form-label space  text-end">Age as on 1st June</label>
                                        <div class="col-sm-4">
                                            <input type="text" asp-for="Age" class="form-control" id="txtAge" autocomplete="off" />
                                        </div>
                                    </div>
                                    <div class="mb-3 row">
                                        <label class="col-sm-4 col-form-label required space text-end">Gender </label>
                                        <div class="col-sm-6">
                                            <div class="col mt-2">
                                                <div class="form-check form-check-inline">
                                                    <input asp-for="Gender" class="form-check-input check" id="radioinline1" type="radio" name="Gnd" value="0">
                                                    <label class="form-check-label" for="radioinline1">Male</label>
                                                </div>
                                                <div class="form-check form-check-inline">
                                                    <input asp-for="Gender" class="form-check-input check" id="radioinline2" type="radio" name="Gnd" value="1">
                                                    <label class="form-check-label" for="radioinline2"> Female</label>
                                                </div>
                                                <span asp-validation-for="Gender" class="text-danger"></span>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="mb-3 row">
                                        <label class="col-sm-4 col-form-label space  text-end">Place of Birth </label>
                                        <div class="col-sm-4">
                                            <input type="text" asp-for="Placeofbirth" class="form-control" id="txtPlaceofbirth" autocomplete="off" />
                                        </div>
                                    </div>
                                    <div class="mb-3 row">
                                        <label class="col-sm-4 col-form-label space required text-end">Applying for Class </label>
                                        <div class="col-sm-4">
                                     
                                            @Html.DropDownListFor(x => x.ClassId, new SelectList(ViewBag.Classnames, "Value", "Text"), "-------select-------",
                                           new { @class = "select2 form-select select2-hidden-accessible", @id = "ddlAdminssionForClass", @style = "color:black;", required = "required" })
                                    
                                            @*<select asp-for="SubclassificationName" class="form-select" id="ddlAdminssionForClass"></select>*@
                                            <span asp-validation-for="ClassId" class="text-danger"></span>
                                        </div>
                                    </div>
                                    <div class="mb-3 row">
                                        <label class="col-sm-4 col-form-label space  text-end"> Name & Place of the previous school </label>
                                        <div class="col-sm-4">
                                            <textarea asp-for="PreviousSchool" class="form-control" id="txtPreviousSchool" autocomplete="off"></textarea>
                                        </div>
                                    </div>
                                    <div class="mb-3 row">
                                        <label class="col-sm-4 col-form-label space  text-end"> Class Studied </label>
                                        <div class="col-sm-4">
                                            <input type="text" asp-for="PreviousClass" class="form-control" id="txtClassstudiedat" autocomplete="off" />
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-12 col-lg-12 mb" id="Fatherdetails">
                <div class="card mb-4">
                    <div class="card-header d-flex align-items-center justify-content-between">
                        <h5 class="mb-0">
                            <span>
                                FATHER DETAILS
                            </span>
                        </h5> <small class="text-muted float-end">* INDICATES MANDATORY FIELD</small>
                    </div>
                    <div class="card-body">
                        <div class="row">
                            <div class="col">
                                <div class="row mb-3">
                                    <div class="mb-3 row">
                                        <label class="col-sm-4 col-form-label space required text-end">Father's Name</label>
                                        <div class="col-sm-4">
                                            <input type="text" asp-for="FatherName" class="col-sm-3 form-control" id="txtFatherName" autocomplete="off" />
                                            <span asp-validation-for="FatherName" class="text-danger"></span>
                                        </div>
                                    </div>
                                    <div class="mb-3 row">
                                        <label class="col-sm-4 col-form-label space text-end">Educational Qualifications</label>
                                        <div class="col-sm-4">
                                            <input type="text" asp-for="FatherQualification" class="form-control" id="txtFatherQualification" autocomplete="off" />
                                        </div>
                                    </div>
                                    <div class="mb-3 row">
                                        <label class="col-sm-4 col-form-label space  text-end">Occupation </label>
                                        <div class="col-sm-4">
                                            <input type="text" asp-for="FatherOccupation" class="form-control" id="txtFatherOccupation" autocomplete="off" />
                                        </div>
                                    </div>
                                    <div class="mb-3 row">
                                        <label class="col-sm-4 col-form-label space  text-end">Designation  </label>
                                        <div class="col-sm-4">
                                            <input type="text" asp-for="FatherDesignation" class="form-control" id="txtFatherDesignation" autocomplete="off" />
                                        </div>
                                    </div>
                                    <div class="mb-3 row">
                                        <label class="col-sm-4 col-form-label space  text-end">No. of years service in the present Organization </label>
                                        <div class="col-sm-4">
                                            <input type="text" asp-for="FatherServiceyears" class="form-control" id="txtFatherServiceyears" autocomplete="off" />
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-12 col-lg-12 mb" id="Motherdetails">
                <div class="card mb-4">
                    <div class="card-header d-flex align-items-center justify-content-between">
                        <h5 class="mb-0">
                            <span>
                                MOTHER DETAILS
                            </span>
                        </h5> <small class="text-muted float-end">* INDICATES MANDATORY FIELD</small>
                    </div>
                    <div class="card-body">
                        <div class="row">
                            <div class="col">
                                <div class="row mb-3">
                                    <div class="mb-3 row">
                                        <label class="col-sm-4 col-form-label space required text-end">Mother's Name</label>
                                        <div class="col-sm-4">
                                            <input type="text" asp-for="MotherName" class="col-sm-3 form-control" id="txtMotherName" autocomplete="off" />
                                            <span asp-validation-for="MotherName" class="text-danger"></span>
                                        </div>
                                    </div>
                                    <div class="mb-3 row">
                                        <label class="col-sm-4 col-form-label space text-end">Educational Qualifications</label>
                                        <div class="col-sm-4">
                                            <input type="text" asp-for="MotherQualification" class="form-control" id="txtMotherQualification" autocomplete="off" />
                                        </div>
                                    </div>
                                    <div class="mb-3 row">
                                        <label class="col-sm-4 col-form-label space  text-end">Occupation  </label>
                                        <div class="col-sm-4">
                                            <input type="text" asp-for="MotherOccupation" class="form-control" id="txtMotherOccupation" autocomplete="off" />
                                        </div>
                                    </div>
                                    <div class="mb-3 row">
                                        <label class="col-sm-4 col-form-label space  text-end">Designation   </label>
                                        <div class="col-sm-4">
                                            <input type="text" asp-for="MotherDesignation" class="form-control" id="txtMotherDesignation" autocomplete="off" />
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-12 col-lg-12 mb" id="Addressdetails">
                <div class="card mb-4">
                    <div class="card-header d-flex align-items-center justify-content-between">
                        <h5 class="mb-0">
                            <span>
                                ADDRESS DETAILS
                            </span>
                        </h5> <small class="text-muted float-end">* INDICATES MANDATORY FIELD</small>
                    </div>
                    <div class="card-body">
                        <div class="row">
                            <div class="col">
                                <div class="row mb-3">
                                    <div class="mb-3 row">
                                        <label class="col-sm-4 col-form-label space text-end">Add Line 1</label>
                                        <div class="col-sm-4">
                                            <input type="text" asp-for="AddressLine1" class="col-sm-3 form-control" id="txtAddressLine1" autocomplete="off" />
                                        </div>
                                    </div>
                                    <div class="mb-3 row">
                                        <label class="col-sm-4 col-form-label space text-end">Add Line 2</label>
                                        <div class="col-sm-4">
                                            <input type="text" asp-for="AddressLine2" class="form-control" id="txtAddressLine2" autocomplete="off" />
                                        </div>
                                    </div>
                                    <div class="mb-3 row">
                                        <label class="col-sm-4 col-form-label space  text-end">City</label>
                                        <div class="col-sm-4">
                                            <input type="text" asp-for="City" class="form-control" id="txtCity" autocomplete="off" />
                                        </div>
                                    </div>
                                    <div class="mb-3 row">
                                        <label class="col-sm-4 col-form-label required space  text-end">Country</label>
                                        <div class="col-sm-4">
                                            <select asp-for="CountryName" id="ddlCountry" class="select2 form-select select2-hidden-accessible" style="color: black;" required="required">
                                                <option value="">-------select-------</option>
                                                @foreach (var country in ViewBag.CountryNames)
                                                {
                                                    @if (ViewBag.EditMode)
                                                    {
                                                        @if (Model.CountryName != null)
                                                        {
                                                            if (country.Text == Model.CountryName)
                                                            {
                                                                <option value="@country.Value" selected="selected">@country.Text</option>
                                                            }
                                                            else
                                                            {
                                                                <option value="@country.Value">@country.Text</option>
                                                            }
                                                        }
                                                        else
                                                        {
                                                            <option value="@country.Value">@country.Text</option>
                                                        }
                                                    }
                                                    else
                                                    {
                                                        <option value="@country.Value">@country.Text</option>
                                                    }
                                                }
                                            </select>
                                        </div>
                                    </div>
                                    <div class="mb-3 row">
                                        <label class="col-sm-4 col-form-label required space text-end">State</label>
                                        <div class="col-sm-4">
                                            <select asp-for="StateName" class="form-select" id="ddlState">
                                                <option value="">-------select-------</option>
                                                @foreach (var State in ViewBag.StateNames)
                                                {
                                                    @if (ViewBag.EditMode)
                                                    {
                                                        @if (Model.StateName != null)
                                                        {
                                                            if (State.Text == Model.StateName)
                                                            {
                                                                <option value="@State.Value" selected="selected">@State.Text</option>
                                                            }
                                                            else
                                                            {
                                                                <option value="@State.Value">@State.Text</option>
                                                            }
                                                        }
                                                        else
                                                        {
                                                            <option value="@State.Value">@State.Text</option>
                                                        }
                                                    }
                                                    else
                                                    {
                                                        <option value="@State.Value">@State.Text</option>
                                                    }
                                                }
                                            </select>
                                        </div>
                                    </div>
                                    <div class="mb-3 row">
                                        <label class="col-sm-4 col-form-label space  text-end">Zip</label>
                                        <div class="col-sm-4">
                                            <input type="text" asp-for="Zip" class="form-control" id="txtZip" autocomplete="off" />
                                            <span asp-validation-for="Zip" class="text-danger"></span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-12 col-lg-12 mb" id="Contactdetails">
                <div class="card mb-4">
                    <div class="card-header d-flex align-items-center justify-content-between">
                        <h5 class="mb-0">
                            <span>
                                CONTACT DETAILS
                            </span>
                        </h5> <small class="text-muted float-end">* INDICATES MANDATORY FIELD</small>
                    </div>
                    <div class="card-body">
                        <div class="row">
                            <div class="col">
                                <div class="row mb-3">
                                    <div class="mb-3 row">
                                        <label class="col-sm-4 col-form-label space required text-end">Contact Mobile No</label>
                                        <div class="col-sm-4">
                                            <input type="text" asp-for="ParentMobileNumber" class="form-control" id="txtParentMobileNumber" autocomplete="off">
                                            <span asp-validation-for="ParentMobileNumber" class="text-danger"></span>
                                        </div>
                                    </div>
                                    <div class="mb-3 row">
                                        <label class="col-sm-4 col-form-label space text-end">E-mail ID</label>
                                        <div class="col-sm-4">
                                            <input type="email" asp-for="ParentEmail" class="form-control" id="txtParentEmail" autocomplete="off" />
                                            <span asp-validation-for="ParentEmail" class="text-danger"></span>
                                            <span id="emailError" class="text-danger"></span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row justify-content-center text-center">
            <div class="col-sm-12">
                <button type="submit"  class="btn btn-success waves-effect waves-light" id="Savebtn" value="Submit">Submit</button>
            </div>
        </div>
    </form>
</body>
</html>
<script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="https://cdn.datatables.net/1.11.5/js/jquery.dataTables.min.js"></script>
<script src="~/Datatable/jquery.dataTables.min.js"></script>

<script src="~/lib/jquery/dist/jquery.js"></script>
<script src="~/lib/jquery/dist/jquery.min.js"></script>

<script src="~/lib/jquery-validation/dist/jquery.validate.js"></script>
@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
<script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js" type="text/javascript"></script>

@*<script src="~/js/CommonScripts.js"></script>*@
<script src="~/js/Admissionprocess_1.js"></script>

