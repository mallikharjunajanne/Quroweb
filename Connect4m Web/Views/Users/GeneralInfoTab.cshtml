
@model Connect4m_Web.Models.ManageUsersModel

@{
    ViewData["Title"] = "GeneralInfoTab";
    Layout = null;
    var UpdateIdentification = ViewBag.UpdateIdentification;
}


<input type="hidden" value="@UpdateIdentification" id="BtnUpdateIdentification" />
@if (UpdateIdentification == "UpdateDetails")
{
        <button class="btn rounded-pill btn-outline-dark waves-effect float-end" style="display:none;" id="BtnPreview" onclick="Preview_CalingFunction('@Model.UserId', this)">
            Preview
        </button>
    
        }
        <div id="DivGeneralInfoTab">
            <form id="FmGeneralInfoTab" class="needs-validation was-validated" novalidate>

                @*<div class="form-group col-12 passwordfor"><span class="ErrorMessageSpan text-danger" id="Main_Span_Error"></span></div>*@
                <div class=" passwordfor"><span class="ErrorMessageSpan text-danger" id="Main_Span_Error"></span></div>
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <input type="hidden" asp-for="UserId" id="HdnUserId" />
                @*<input type="hidden" asp-for="InstanceID" id="HdnInstanceID" />*@
                @*=============================Save Users Card=================================*@
                <div class="card mb-4">
                    <div class="card-header d-flex align-items-center justify-content-between">
                        <h5 class="mb-0">
                            <span id="SpnCardHeaderName">
                                SAVE USERS
                            </span>
                        </h5><small class="required float-end"> INDICATES REQUIRED FIELDS</small>
                    </div>
                    <div class="card-body">
                        <div class="row">

                            <div class="col">
                                @*----------------------*@

                                <div class="mb-3 row">
                                    <label asp-for="RoleId" class="col-sm-2 col-form-label  text-end required" for="basic-default-name"></label>
                                    <div class="col-sm-4">
                                        <div class="position-relative">
                                            @Html.DropDownListFor(x => x.RoleId,
                new SelectList(ViewBag.DdlInstanceRole, "Value", "Text"), "-------select-------",
                new { @class = "select2 form-select select2-hidden-accessible", @id = "DdlRoleIdInGeneralTab", @onchange = "RoleOnChangeFunctionInGeneralTab(this.id)", @required = "required" })

                                            <span asp-validation-for="RoleId" class="text-danger"></span>
                                        </div>
                                    </div>

                                    @*<label class="col-sm-2 col-form-label  text-end" for="basic-default-name"></label>*@

                                    <div class="col-sm-6 ">
                                        @*<img src="@ViewBag.PhotoNameFullName" style=" display: none; height: 100px; width: 100px; border-width: 0px;" />*@
                                        @*@if (ViewBag.PhotoName != "" && UpdateIdentification == "UpdateDetails")*@
                                        @if (UpdateIdentification == "UpdateDetails")
                                        {
                                            <img src="@ViewBag.PhotoNameFullName" alt="" id="ImgPhotoDisplay" />
                                            @*<img src="~/Images/No imageAvailable.gif" alt="" id="ImgNophotoAvailable" style="height: 100px; width: 100px; border-width: 0px; margin-left: 120%;display:none" />*@
                                            @*<img src="~/Images/No imageAvailable.gif" alt="" id="ImgNophotoAvailable" style="height: 100px; width: 100px; border-width: 0px; margin-left: 120%;display:none" />*@
                                        }
                                        @*else if (UpdateIdentification == "UpdateDetails")
                                            {
                                                <img src="@ViewBag.PhotoNameFullName" id="ImgPhotoDisplay" alt="" style="height: 100px; width: 100px; border-width: 0px; margin-left: 120%; display: none" />
                                                <img src="~/Images/No imageAvailable.gif" id="ImgNophotoAvailable" alt="" style="height: 100px; width: 100px; border-width: 0px; margin-left: 120%;" />

                                            }*@
                                        @if (ViewBag.FatherName != "" && ViewBag.FatherName != null && UpdateIdentification == "UpdateDetails")
                                        {
                                            <label asp-for="ParentName" class="control-label space">Father Name </label><span class="text-danger">@ViewBag.FatherName</span>
                                        }<br />
                                        @if (ViewBag.MotherFirstName != "" && ViewBag.MotherFirstName != null && UpdateIdentification == "UpdateDetails")
                                        {
                                            <label asp-for="MotherFirstName" class="control-label space ">Mother Name </label><span>@ViewBag.MotherFirstName</span>
                                        }
                                    </div>
                                </div>
                                @*----------------------*@

                                <div class="mb-3 row">
                                    <label asp-for="DesignationId" class="col-sm-2 col-form-label  text-end  ClsDivTeachers" for="basic-default-name"></label>
                                    <div class="col-sm-4 ClsDivTeachers">
                                        <div class="position-relative">
                                            @Html.DropDownListFor(x => x.DesignationId, new SelectList(ViewBag.DdlDesignation, "Value", "Text"), "-------select-------", new { @class = "select2 form-select select2-hidden-accessible ClsClear" })
                                            @*<span asp-validation-for="DesignationId" class="text-danger"></span>*@
                                        </div>
                                    </div>

                                </div>
                                @*----------------------*@

                                <div class="mb-3 row">
                                    <label asp-for="Qualification" class="col-sm-2 col-form-label  text-end ClsDivTeachers" for="basic-default-name"></label>
                                    <div class="col-sm-4 ClsDivTeachers">
                                        <input type="text" asp-for="Qualification" class="form-control ClsClear" placeholder="Qualification">
                                        <span asp-validation-for="Qualification" class="text-danger"></span>
                                    </div>
                                </div>

                                @*----------------------*@
                                <div class="mb-3 row">
                                    <label asp-for="Batch" class="col-sm-2 col-form-label  text-end ClsSessionStudent" for="basic-default-name"></label>
                                    <div class="col-sm-4 ClsSessionStudent">
                                        <div class="position-relative">
                                            @Html.DropDownListFor(x => x.Batch, new SelectList(ViewBag.DdlSessionList, "Value", "Text"), "-------select-------", new { @class = "select2 form-select select2-hidden-accessible  ClsClear" })
                                        </div>
                                    </div>
                                    <label asp-for="Photo" class="col-sm-2 col-form-label  text-end " for="basic-default-name"></label>
                                    <div class="col-sm-4 ">
                                        @if (ViewBag.PhotoName != "" && ViewBag.PhotoName != null && UpdateIdentification == "UpdateDetails")
                                        {
                                            <span id="SpnPhotoName" class="ClsFileName">@ViewBag.PhotoName</span><a class=" btn rounded-pill btn-outline-danger waves-effect ClsFileName" onclick="DeletePhoto_CallingFunction()">Delete</a>
                                            <input type="file" asp-for="Photo" id="FlPhoto" class="form-control" style="display:none" />
                                        }
                                        else if (UpdateIdentification == "UpdateDetails")
                                        {
                                            <span style="display:none" class="ClsFileName" id="SpnPhotoName">@ViewBag.PhotoName</span><a style="display: none !important" class="btn rounded-pill btn-outline-danger waves-effect ClsFileName " onclick="DeletePhoto_CallingFunction()">Delete</a>
                                            <input type="file" asp-for="Photo" id="FlPhoto" class="form-control" />
                                        }
                                        else
                                        {
                                            <input type="file" asp-for="Photo" id="FlPhoto" class="form-control" />
                                        }
                                        <input type="hidden" asp-for="PhotoSize" readonly />
                                        <input type="hidden" asp-for="PhotoName" readonly />
                                    </div>
                                </div>
                                @*----------------------*@


                                <div class="mb-3 row">
                                    <label asp-for="InstanceUserCode" id="LblInstanceUserCodeGeneralTab" class="col-sm-2 col-form-label  text-end ClsDivTeachers" for="basic-default-name">Reg. No./Emp ID</label>
                                    <div class="col-sm-4 ClsDivTeachers">
                                        <input type="text" asp-for="InstanceUserCode" class="form-control ClsClear" placeholder="Reg. No./Emp ID">
                                        <span asp-validation-for="InstanceUserCode" class="text-danger"></span>
                                    </div>
                                </div>
                                @*----------------------*@
                                <div class="mb-3 row">

                                    @*<label asp-for="FirstName"  class="col-sm-2 col-form-label  text-end required" for="basic-default-name"></label>
                                        <div class="col-sm-4 ">
                                            <input type="text" asp-for="FirstName" class="form-control" placeholder="Reg. No./Emp ID">
                                            <span asp-validation-for="FirstName" class="text-danger"></span>
                                        </div>*@
                                    <label asp-for="FirstNameInGeneral" class="col-sm-2 col-form-label  text-end required" for="basic-default-name"></label>
                                    <div class="col-sm-4 ">
                                        <input type="text" asp-for="FirstNameInGeneral" class="form-control" placeholder="First Name" required>
                                        <span asp-validation-for="FirstNameInGeneral" class="text-danger"></span>
                                    </div>

                                    <label asp-for="MiddleName" class="col-sm-2 col-form-label  text-end " for="basic-default-name"></label>
                                    <div class="col-sm-4 ">
                                        <input type="text" asp-for="MiddleName" class="form-control " placeholder="Middle Name">
                                        <span asp-validation-for="MiddleName" class="text-danger"></span>
                                    </div>
                                </div>

                                @*----------------------*@
                                <div class="mb-3 row">

                                    <label asp-for="LastName" class="col-sm-2 col-form-label  text-end " for="basic-default-name"></label>
                                    <div class="col-sm-4 ">
                                        <input type="text" asp-for="LastName" class="form-control" placeholder="Last Name">
                                        <span asp-validation-for="LastName" class="text-danger"></span>
                                    </div>

                                    <label asp-for="DateOfJoining" class="col-sm-2 col-form-label  text-end required" for="basic-default-name"></label>
                                    <div class="col-sm-4 ">
                                        <input type="date" id="DtDateOfJoining" asp-for="DateOfJoining" class="form-control" oninput="compareDatesNotGreaterThanTodayById(this.getAttribute('id'), 'Date Of Joining')" placeholder="Date Of Joining" required />
                                        <span asp-validation-for="DateOfJoining" class=" text-danger"></span>
                                        <span class="compare text-danger"></span>
                                    </div>
                                </div>

                                @*----------------------*@

                                <div class="mb-3 row">
                                    <label asp-for="Nationality" class="col-sm-2 col-form-label  text-end " for="basic-default-name"></label>
                                    <div class="col-sm-4 ">
                                        <input type="text" asp-for="Nationality" class="form-control " placeholder="Nationality">
                                    </div>
                                    <label asp-for="AdmissionNumberNew" class="col-sm-2 col-form-label  text-end required ClsSessionStudent" for="basic-default-name"></label>
                                    <div class="col-sm-4 ClsSessionStudent">
                                        <input asp-for="AdmissionNumberNew" class="form-control ClsClear" placeholder="Admission Number" required />
                                        <span asp-validation-for="AdmissionNumberNew" class="text-danger"></span>
                                    </div>
                                </div>

                                @*----------------------*@
                                <div class="mb-3 row">
                                    <label asp-for="IsUserJoined" class="col-sm-2 col-form-label  text-end ClsSessionStudent" for="basic-default-name"></label>
                                    <div class="col-sm-4 ClsSessionStudent">
                                        <input type="checkbox" asp-for="IsUserJoined" id="ChkIsUserJoined" class="form-check-input ClsClear" disabled />
                                    </div>
                                    <label asp-for="BloodGroup" class="col-sm-2 col-form-label  text-end " for="basic-default-name"></label>
                                    <div class="col-sm-4 ">
                                        <div class="position-relative">
                                            <select id='DdlPickUpRoute' asp-for="BloodGroup" class="select2 form-select select2-hidden-accessible" asp-items="@(new SelectList(ViewBag.DdlBloodGroupList, "Value", "Text"))">
                                                <option value=''>---------Select---------</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>


                                @*----------------------*@
                                <div class="mb-3 row">
                                    <label asp-for="PlaceOfBirth" class="col-sm-2 col-form-label  text-end " for="basic-default-name"></label>
                                    <div class="col-sm-4 ">
                                        <input type="text" asp-for="PlaceOfBirth" class="form-control " placeholder="Place Of Birth">
                                        <span asp-validation-for="PlaceOfBirth" class="text-danger"></span>
                                    </div>
                                    <label asp-for="CollegeCode" class="col-sm-2 col-form-label  text-end " for="basic-default-name"></label>
                                    <div class="col-sm-4 ">
                                        <input type="text" asp-for="CollegeCode" class="form-control " placeholder="School Code">
                                        <span asp-validation-for="CollegeCode" class="text-danger"></span>
                                    </div>
                                </div>

                                @*----------------------*@
                                <div class="mb-3 row">
                                    <label asp-for="Gender" class="col-sm-2 col-form-label  text-end required" for="basic-default-name"></label>
                                    <div class="col-sm-4">
                                        <div class="col mt-2">
                                            <div class="form-check form-check-inline">
                                                <input class="form-check-input " asp-for="Gender" id="Male" type="radio" name="Gender" value="1" required />
                                                <label class="form-check-label" for="Male">Male</label>
                                            </div>
                                            <div class="form-check form-check-inline">
                                                <input class="form-check-input " asp-for="Gender" id="Female" type="radio" name="Gender" value="0" required />
                                                <label class="form-check-label" for="Female"> Female</label>
                                            </div>
                                            <div class="form-check form-check-inline">
                                                <input class="form-check-input " asp-for="Gender" id="Others" type="radio" name="Gender" value="" required />
                                                <label class="form-check-label" for="Others"> Others</label>
                                            </div>
                                            <span asp-validation-for="Gender" class="text-danger"></span>
                                        </div>
                                    </div>
                                    <label asp-for="FeeConcedingTypeId" class="col-sm-2 col-form-label  text-end ClsDivTeachers" for="basic-default-name"></label>
                                    <div class="col-sm-4 ClsDivTeachers">
                                        <div class="position-relative">
                                            <select id='DdlPickUpStop' asp-for="FeeConcedingTypeId" class="select2 form-select select2-hidden-accessible ClsClear" asp-items="@(new SelectList(ViewBag.DdlFeeConcedingTypesList , "Value", "Text"))">
                                                <option value=''>---------Select---------</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>

                                @*----------------------*@
                                <div class="mb-3 row">
                                    <label asp-for="ADMISSIONCATEGORY" class="col-sm-2 col-form-label  text-end " for="basic-default-name"></label>
                                    <div class="col-sm-4 ">
                                        <div class="position-relative">
                                            <select asp-for="ADMISSIONCATEGORY" multiple="multiple" class="select2 form-select select2-hidden-accessible" asp-items="@(new SelectList(""))">
                                                <option value="1" title="General">General</option>
                                                <option value="2" title="EWS">EWS</option>
                                                <option value="3" title="Sibling">Sibling</option>
                                                <option value="4" title="Transfer">Transfer</option>
                                                <option value="5" title="Staff">Staff</option>
                                                <option value="6" title="Management">Management</option>
                                                <option value="7" title="Under Privileged">Under Privileged</option>
                                                <option value="8" title="Special Education Needs">Special Education Needs</option>
                                                <option value="9" title="Disadvantage Group">Disadvantage Group</option>
                                            </select>
                                        </div>
                                    </div>
                                    <label asp-for="ICNumber" class="col-sm-2 col-form-label  text-end " for="basic-default-name"></label>
                                    <div class="col-sm-4 ">
                                        <input asp-for="ICNumber" class="form-control" placeholder="Aadhaar Number" />
                                        <span asp-validation-for="ICNumber" class="text-danger"></span>
                                    </div>
                                </div>

                                @*----------------------*@


                            </div>

                        </div>
                    </div>
                </div>

                @*--- PAY ROLE INFORMATION Card----*@
                <div class="card mb-4 ClsDivTeachers">

                    <div class="card-header d-flex align-items-center justify-content-between">
                        <h5 class="mb-0">
                            <span>
                                PAY ROLE INFORMATION
                            </span>
                        </h5><small class="required float-end"> INDICATES REQUIRED FIELDS</small>
                    </div>
                    <div class="card-body">
