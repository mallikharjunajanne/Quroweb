@model IEnumerable<Connect4m_Web.Models.LMSproperties.ViewLectureDocs>

@{
    ViewData["Title"] = "ViewLectureDocsSub";
}
<link href="~/css/Viewlecturedocs.css" rel="stylesheet" />
<h5>VIEW LECTURES / DOCS</h5>
<label id="VLD_Backtoprogram"> &lt;&lt; Back To Programs </label>
<div class="row">
    @foreach (var item in Model)
    {
        <div class="col-sm-4">
            <div class="card">

                <div class="card-header pb-0">


                </div>
                <div class="card-body">
                    <div class="default-according style-1" id="accordionoc">
                        <div class="card">
                            <div class="card-header bg-primary">
                                <h5 class="mb-0">
                                    <button id="collapse" class="btn btn-link text-white" data-bs-toggle="collapse" data-bs-target="#collapseicon" aria-expanded="true" aria-controls="collapse11">@item.SubjectName</button>
                                </h5>
                            </div>
                            <div class="collapse show" id="collapseicon1" aria-labelledby="collapseicon" data-bs-parent="#accordionoc">

                                <div class="row padding-search">
                                    <div class="row">
                                        <div style="margin-top: 12%; margin-bottom: 12%;">


                                            <img src="~/Images/Book.png" style="height: 70px; width: 60px; border-width: 0px;" />
                                            <label style="margin-left: 19%;">   @item.ClassTeacherName</label>
                                        </div>

                                    </div>


                                </div>
                                <div id="VLD_ProgressDetails_sub">
                                    <input type="text" value="@item.InstanceClassificationId" id="VLD_Instanceclassficationid_sub" hidden style="display:none" />
                                    <input type="text" value="@item.InstanceSubClassificationId " id="VLD_InstanceSubClassificationId_sub" hidden style="display:none" />
                                    <input type="text" value="@item.SubjectId" id="VLD_SubjectId_sub" hidden style="display:none" />
                                    <label id="ProgramDetails_sub">Program Details >></label>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    }


</div>
<script src="~/lib/jquery/dist/jquery.js"></script>

<script src="~/js/ViewLecturedocs.js"></script>