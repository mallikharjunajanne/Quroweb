@*@{
        string base64Data = ViewData["Base64Data"] as string;
    }

    <script>
        function openFileInNewWindow() {
            var dataUrl = "data:application/vnd.ms-excel," + '@base64Data';

            var link = document.createElement('a');
            link.href = dataUrl;
            link.target = '_blank';
            link.click();
        }
    </script>

    <button onclick="openFileInNewWindow()">Open File</button>*@
