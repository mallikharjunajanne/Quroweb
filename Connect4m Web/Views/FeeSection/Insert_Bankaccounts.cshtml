@model Connect4m_Web.Models.Attendenceproperites.BankAccounts
@{
    ViewData["Title"] = "Manage feeterms Insert";
    Layout = null;
    //Layout = "~/Views/Shared/_Layout.cshtml";
}



<div class="col-md-12 col-lg-12 mb-3">
    <div class="card mb-4">
        <div class="card-header d-flex align-items-center justify-content-between">
            <h5 class="mb-0"><span id="Heading_sptxt">SAVE BANK DETAILS</span></h5><small class="text-muted float-end">* INDICATES REQUIRED FIELDS</small>
        </div>
        <div class="card-body">
            <form id="Bankaccountsform" enctype="multipart/form-data">
                <div class="row">
                    <div class="row mb-3">
                        <div class="row mb-3">
                            <label class="col-sm-4 col-form-label space text-end required" for="basic-default-name">Bank Name</label>
                            <div class="col-sm-4">
                                <input type="text" asp-for="BankName" class="form-control" id="BankNametxtid" />
                                <span asp-validation-for="BankName" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="mb-3 row">
                            <label class="col-sm-4 col-form-label space text-end required" for="basic-default-name">Account Number</label>
                            <div class="col-sm-4">
                                <input type="text" asp-for="AccountNumber" class="form-control" id="AccountNumbertxtid" />
                                <span asp-validation-for="AccountNumber" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="row mb-3">
                            <label class="col-sm-4 col-form-label space text-end required" for="basic-default-name">Branch Code</label>
                            <div class="col-sm-4">
                                <input type="text" asp-for="BranchCode" class="form-control" id="BranchCodetxtid" />
                                <span asp-validation-for="BranchCode" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="row mb-3">
                            <label class="col-sm-4 col-form-label space text-end" for="basic-default-name">IFSC Code</label>
                            <div class="col-sm-4">
                                <input type="text" asp-for="IFSCCode" id="IFSCCodetxtid" class="form-control">
                                <span asp-validation-for="IFSCCode" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="row mb-3">
                            <label class="col-sm-4 col-form-label space text-end" for="basic-default-name">Bank Branch Address</label>
                            <div class="col-sm-4">
                                 <textarea asp-for="Address" id="Addresstxtid" class="form-control" rows="4" cols="20" > </textarea>
                            </div>
                        </div>
                        <div class="row mb-3">
                            <label class="col-sm-4 col-form-label space text-end" for="basic-default-name">Description </label>
                            <div class="col-sm-4">
                                <textarea asp-for="Description" id="Descriptiontxtid" class="form-control" rows="4" cols="20" > </textarea>
                            </div>
                        </div>
                    </div>
                    <div class="row justify-content-center text-center">
                        <div class="col-sm-12">
                            <div class="col-sm-12">
                                <button type="button" class="btn btn-info waves-effect waves-light" onclick="previouspages('ManageFeeTerms',null)">Previous Page</button>
                                <button type="reset" class="btn btn-warning waves-effect waves-light" onclick="clearForm('Feetermform')" id="btnClear">Clear</button>
                                <button type="button" class="btn btn-dark waves-effect waves-light" id="Svbacktosearchbtn">Back To Search</button>
                                <button type="submit" class="btn btn-success waves-effect waves-light" id="savebtn">Save</button>
                                <button type="button" class="btn btn-primary waves-effect waves-light" onclick="nextpages('ManageFeeDetails',null)">Next Page</button>
                            </div>
                        </div>
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>



<script src="~/lib/jquery-validation/dist/jquery.validate.js"></script>
@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
<script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js" type="text/javascript"></script>

<script src="~/js/Managebankaccounts1.js"></script>




