@model IEnumerable<Connect4m_Web.Models.Attendenceproperites.Termwisechallandetails>

@{
    Layout = null;
}


<table class="table table-hover table-bordered" id="Installmentfeedetailstblid">
    <thead>
        <tr>
            <th>Date</th>
            <th>Particulars</th>
            <th></th>
            <th></th>
            <th>Balance</th>
        </tr>
    </thead>
    <tbody class="table-border-bottom-0">
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Createdate</td>
                <td>
                    <a href="javascript:void(0)" data-id="@item.modename" onclick="Getviewreport('@item.UserReceiptGenerationID', '@item.UserId')">
                        @item.modename
                    </a>
                </td>
                <td>@item.typename</td>
                <td>@item.Amount</td>
                <td>
                    @item.balance
                    <span value="@item.UserId" title="@item.Createdate" name="@ViewBag.username"></span>
                </td>
            </tr>
        }
    </tbody>
</table>



