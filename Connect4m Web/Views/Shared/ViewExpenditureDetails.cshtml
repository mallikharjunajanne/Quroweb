                            <div class="row padding-search">
                                <div class="row">

                                    <form id="InsertSEM" enctype="multipart/form-data">
                                        <div asp-validation-summary="ModelOnly" class="text-danger"></div>

                                        <div class="row">
                                            <input asp-for="GovFundId" class="form-control" type="number" disabled id="GovFundId_Sem" hidden />

                                            @*<input asp-for="InstanceSubjectId" class="form-control" id="formInstanceSubjectId" hidden />*@


                                            <div class="form-group col-2">


                                                <label asp-for="TypeofExpenditure" class="control-label  space"></label>
                                                @*<i class="fa fa-asterisk" style="font-size:11px;color:red"></i>*@

                                            </div>
                                            <div class="form-group col-4">

                                                <div id="Drop_Format_SEM_1">
                                                    @Html.DropDownListFor(
x => x.TypeofExpenditure,
 new SelectList(ViewBag.FoundSource, "Value", "Text"),
 " ", new { @class = "form-control readonly", @id = "dropdown_FoundSource_SEM", @style = "color:black;", @disabled = true })

                                                </div>
                                                <span asp-validation-for="TypeofExpenditure" class="text-danger"></span>

                                            </div>
                                            <div class="form-group col-2">
                                                <label asp-for="VendorName" class="control-label space"></label>

                                            </div>
                                            <div class="form-group col-4">

                                                <input asp-for="VendorName" class="form-control" id="VendorName_SEM" disabled />

                                            </div>

                                            <div class="form-group col-2">


                                                <label asp-for="YearId" class="control-label  space"></label>
