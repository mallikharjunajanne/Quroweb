
@model Connect4m_Web.Models.ManageUsersModel

@{
    ViewData["Title"] = "ShowProfile";

    var UpdateIdentification = ViewBag.UpdateIdentification;
    var Text = "";
}
@*<link href="~/Datatable/jquery.dataTables.css" rel="stylesheet" />*@

<!DOCTYPE html>
<html>
<head>

</head>
<body>
    <div class="flex-grow-1 container-p-y container-fluid">

        <h4 class="py-3 mb-4"><span class="text-muted fw-light">Admin Module /</span>MY PROFILE</h4>

        <div class="row mb-2">
            <div class="col-md-12 col-lg-12 mb-3">
                @*<div id="DivScreenName" data-mydata="@ViewData["Title"]"></div>*@

                <input type="hidden" value="@UpdateIdentification" id="BtnUpdateIdentification" />
                <input type="hidden" value="@ViewData["Title"]" id="HdnScreenName" />

                <div id="DivGeneralInfoTab">
                    <form id="FmShowProfile">

                        @*<div class="form-group col-12 passwordfor"><span class="ErrorMessageSpan" id="Main_Span_Error"></span></div>*@
                        <div class=" passwordfor"><span class="ErrorMessageSpan text-danger" id="Main_Span_Error"></span></div>
                        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                        <input type="hidden" asp-for="UserId" id="HdnUserId" />
                        @*<input type="hidden" asp-for="InstanceID" id="HdnInstanceID" />*@
                        @*//Student code*@

                        @if (ViewBag.IsStudentIdentification == "STUDENT")// Student code
                        {

                            @*=================================== Users Details Card==============================*@
                            <div class="card mb-4">
                                <div class="card-header d-flex align-items-center justify-content-between">
                                    <h5 class="mb-0">
                                        <span id="SpnCardHeaderName">
                                            STUDENT DETAILS
                                        </span>
                                    </h5>
                                </div>
                                <div class="card-body">
                                    <div class="row">

                                        <div class="col">
                                            @*----------------------*@
                                            <div class="mb-1 row">
                                                <label asp-for="RoleId" class="col-sm-2 col-form-label  text-end " for="basic-default-name"></label>
                                                <div class="col-sm-4 HideFileds" style="display:none !important">
                                                    <div class="position-relative">
                                                        @Html.DropDownListFor(x => x.RoleId,
                            new SelectList(ViewBag.DdlInstanceRole, "Value", "Text"), "-------select-------",
                            new { @class = "select2 form-select select2-hidden-accessible", @id = "DdlRoleIdInGeneralTab", @onchange = "RoleOnChangeFunctionInGeneralTab(this.id)", @required = "required" })

                                                        <span asp-validation-for="RoleId" class="text-danger"></span>
                                                    </div>
                                                </div>
                                                <div class="col-sm-4">
                                                    <input type="text" readonly="" class="form-control-plaintext" value="@Model.RoleName">
                                                    @*<input type="hidden" asp-for="RoleId" id="DdlRoleId" readonly />*@
                                                </div>

                                                @*<label class="col-sm-2 col-form-label  text-end" for="basic-default-name"></label>*@

                                                <div class="col-sm-6 ">
                                                    @*<img src="@ViewBag.PhotoNameFullName" style=" display: none; height: 100px; width: 100px; border-width: 0px;" />*@
                                                    @*@if (ViewBag.PhotoName != "" && UpdateIdentification == "UpdateDetails")*@
                                                    @if (UpdateIdentification == "UpdateDetails")
                                                    {
                                                        <img src="@ViewBag.PhotoNameFullName" alt="" id="ImgPhotoDisplay" />
                                                        @*<img src="~/Images/No imageAvailable.gif" alt="" id="ImgNophotoAvailable" style="height: 100px; width: 100px; border-width: 0px; margin-left: 120%;display:none" />*@
                                                        @*<img src="~/Images/No imageAvailable.gif" alt="" id="ImgNophotoAvailable" style="height: 100px; width: 100px; border-width: 0px; margin-left: 120%;display:none" />*@
                                                    }
                                                    @*else if (UpdateIdentification == "UpdateDetails")
                                                        {
                                                            <img src="@ViewBag.PhotoNameFullName" id="ImgPhotoDisplay" alt="" style="height: 100px; width: 100px; border-width: 0px; margin-left: 120%; display: none" />
                                                            <img src="~/Images/No imageAvailable.gif" id="ImgNophotoAvailable" alt="" style="height: 100px; width: 100px; border-width: 0px; margin-left: 120%;" />

                                                        }*@
                                                    @if (ViewBag.FatherName != "" && ViewBag.FatherName != null && UpdateIdentification == "UpdateDetails")
                                                    {
                                                        <label asp-for="ParentName" class="control-label space">Father Name </label><span class="text-danger">@ViewBag.FatherName</span>
                                                    }<br />
                                                    @if (ViewBag.MotherFirstName != "" && ViewBag.MotherFirstName != null && UpdateIdentification == "UpdateDetails")
                                                    {
                                                        <label asp-for="MotherFirstName" class="control-label space ">Mother Name </label><span>@ViewBag.MotherFirstName</span>
                                                    }
                                                </div>
                                            </div>

                                            @*----------------------*@
                                            <div class="mb-1 row">
                                                <label asp-for="DesignationId" class="col-sm-2 col-form-label  text-end  ClsDivTeachers" for="basic-default-name"></label>
                                                <div class="col-sm-4 ClsDivTeachers">
                                                    <input type="text" readonly="" class="form-control-plaintext" value="@Model.Designation">
                                                </div>


                                            </div>
                                            @*----------------------*@

                                            <div class="mb-1 row">
                                                <label asp-for="Qualification" class="col-sm-2 col-form-label  text-end ClsDivTeachers" for="basic-default-name"></label>
                                                <div class="col-sm-4 ClsDivTeachers">
                                                    <input type="text" readonly="" class="form-control-plaintext" value="@Model.Qualification">
                                                </div>
                                            </div>
                                            @*----------------------*@
                                            <div class="mb-1 row ">
                                                <label asp-for="Batch" class="col-sm-2 col-form-label  text-end ClsSessionStudent" for="basic-default-name"></label>
                                                <div class="col-sm-4 ClsSessionStudent">
                                                    <input type="text" readonly="" class="form-control-plaintext" value="@Model.Batch">
                                                </div>

                                                <label asp-for="Photo" class="col-sm-2 col-form-label  text-end " for="basic-default-name"></label>
                                                <div class="col-sm-4 ">
                                                    @if (ViewBag.PhotoName != "" && ViewBag.PhotoName != null && UpdateIdentification == "UpdateDetails")
                                                    {
                                                        <span id="SpnPhotoName" class="ClsFileName">@ViewBag.PhotoName</span><a class=" btn rounded-pill btn-outline-danger waves-effect ClsFileName" onclick="DeletePhoto_CallingFunction()">Delete</a>
                                                        <input type="file" asp-for="Photo" id="FlPhoto" class="form-control" style="display:none" />
                                                    }
                                                    else if (UpdateIdentification == "UpdateDetails")
                                                    {
                                                        <span style="display:none" class="ClsFileName" id="SpnPhotoName">@ViewBag.PhotoName</span><a style="display: none !important" class="btn rounded-pill btn-outline-danger waves-effect ClsFileName " onclick="DeletePhoto_CallingFunction()">Delete</a>
                                                        <input type="file" asp-for="Photo" id="FlPhoto" class="form-control" />
                                                    }
                                                    else
                                                    {
                                                        <input type="file" asp-for="Photo" id="FlPhoto" class="form-control" />
                                                    }
                                                    <input type="hidden" asp-for="PhotoSize" readonly />
                                                    <input type="hidden" asp-for="PhotoName" readonly />
                                                </div>
                                            </div>
                                            @*----------------------*@

                                            <div class="mb-1 row">
                                                <label asp-for="InstanceUserCode" id="LblInstanceUserCodeGeneralTab" class="col-sm-2 col-form-label  text-end ClsDivTeachers" for="basic-default-name">Reg. No./Emp ID</label>
                                                <div class="col-sm-4 ClsDivTeachers">
                                                    <input type="text" readonly="" class="form-control-plaintext" value="@Model.InstanceUserCode">
                                                </div>
                                            </div>
                                            @*----------------------*@
                                            <div class="mb-1 row">

                                                <label asp-for="FirstNameInGeneral" class="col-sm-2 col-form-label  text-end " for="basic-default-name"></label>
                                                <div class="col-sm-4 ">
                                                    <input type="text" readonly="" class="form-control-plaintext" value="@Model.FirstNameInGeneral">
                                                </div>

                                                <label asp-for="MiddleName" class="col-sm-2 col-form-label  text-end " for="basic-default-name"></label>
                                                <div class="col-sm-4 ">
                                                    <input type="text" readonly="" class="form-control-plaintext" value="@Model.MiddleName">
                                                </div>
                                            </div>
                                            @*----------------------*@
                                            <div class="mb-1 row">
                                                <label asp-for="LastName" class="col-sm-2 col-form-label  text-end " for="basic-default-name"></label>
                                                <div class="col-sm-4 ">
                                                    <input type="text" readonly="" class="form-control-plaintext" value="@Model.LastName">
                                                </div>

                                                <label asp-for="DateOfJoining" class="col-sm-2 col-form-label  text-end " for="basic-default-name"></label>
                                                <div class="col-sm-4 ">
                                                    <input type="text" readonly="" class="form-control-plaintext" value="@(Model.DateOfJoining != null ? Convert.ToDateTime(Model.DateOfJoining).ToString("dd/MM/yyyy"): string.Empty)">
                                                </div>
                                            </div>
                                            @*----------------------*@

                                            <div class="mb-1 row">
                                                <label asp-for="Nationality" class="col-sm-2 col-form-label  text-end " for="basic-default-name"></label>
                                                <div class="col-sm-4 ">
                                                    <input type="text" readonly="" class="form-control-plaintext" value="@Model.Nationality">
                                                </div>
                                                <label asp-for="AdmissionNumberNew" class="col-sm-2 col-form-label  text-end  ClsSessionStudent" for="basic-default-name"></label>
                                                <div class="col-sm-4 ClsSessionStudent">
                                                    <input type="text" readonly="" class="form-control-plaintext" value="@Model.AdmissionNumberNew">
                                                </div>
                                            </div>
                                            @*----------------------*@

                                            <div class="mb-1 row">
                                                @*<label asp-for="IsUserJoined" class="col-sm-2 col-form-label  text-end ClsSessionStudent" for="basic-default-name"></label>
                                                    <div class="col-sm-4 ClsSessionStudent">
                                                        @if (Model.IsUserJoined == "True")
                                                        {<input type="text" readonly="" class="form-control-plaintext" value="Yes">
                                                        }
                                                        else
                                                        {<input type="text" readonly="" class="form-control-plaintext" value="No">
                                                        }
                                                    </div>*@
                                                <label asp-for="BloodGroup" class="col-sm-2 col-form-label  text-end " for="basic-default-name"></label>
                                                <div class="col-sm-4 ">
                                                    <input type="text" readonly="" class="form-control-plaintext" value="@Model.BloodGroupName">
                                                </div>
                                            </div>
                                            @*----------------------*@
                                            <div class="mb-1 row">
                                                <label asp-for="PlaceOfBirth" class="col-sm-2 col-form-label  text-end " for="basic-default-name"></label>
                                                <div class="col-sm-4 ">
                                                    <input type="text" readonly="" class="form-control-plaintext" value="@Model.PlaceOfBirth">
                                                </div>
                                                <label asp-for="CollegeCode" class="col-sm-2 col-form-label  text-end " for="basic-default-name"></label>
                                                <div class="col-sm-4 ">
                                                    <input type="text" readonly="" class="form-control-plaintext" value="@Model.CollegeCode">
                                                </div>
                                            </div>
                                            @*----------------------*@
                                            <div class="mb-1 row">
                                                <label asp-for="Gender" class="col-sm-2 col-form-label  text-end " for="basic-default-name"></label>
                                                <div class="col-sm-4 ">
                                                    @{ var Gender = "";}
                                                    @if (Model.Gender == "1")
                                                    {
                                                        Gender = "Male";
                                                    }
                                                    else if (Model.Gender == "0")
                                                    {
                                                        Gender = "FeMale";
                                                    }
                                                    else
                                                    {
                                                        Gender = "Others";
                                                    }
                                                    <input type="text" readonly="" class="form-control-plaintext" value="@Gender">
                                                </div>
                                                <label asp-for="ICNumber" class="col-sm-2 col-form-label  text-end " for="basic-default-name"></label>
                                                <div class="col-sm-4 ">
                                                    <input type="text" readonly="" class="form-control-plaintext" value="@Model.ICNumber">
                                                </div>
                                            </div>
                                            @*----------------------*@
                                        </div>
                                    </div>
                                </div>
                            </div>
                            @*===================================PERSONAL INFORMATION Card========================*@
                            <div class="card mb-4">
                                <div class="card-header d-flex align-items-center justify-content-between">
                                    <h5 class="mb-0">
                                        <span>
                                            PERSONAL INFORMATION
                                        </span>
                                    </h5>
                                </div>
                                <div class="card-body">
                                    <div class="row">
                                        <div class="col">
                                            @*----------------------*@
                                            <div class="mb-1 row">
                                                <label asp-for="DOB" class="col-sm-2 col-form-label  text-end " for="basic-default-name"></label>
                                                <div class="col-sm-4 ">
                                                    <input type="date" Id="DOB" asp-for="DOB" class="form-control" oninput="compareDatesNotGreaterThanTodayById(this.getAttribute('id'), 'Date Of Birth')" />
                                                    <span asp-validation-for="DOB" class="text-danger"></span>
                                                    <span class="compare text-danger"></span>
                                                </div>
                                                <label asp-for="ReligionId" class="col-sm-2 col-form-label  text-end " for="basic-default-name"></label>
                                                <div class="col-sm-4 ">
                                                    <input type="text" readonly="" class="form-control-plaintext" value="@Model.ReligionName">
                                                </div>
                                            </div>


                                            @*----------------------*@

                                            <div class="mb-1 row">
                                                <label asp-for="CommunityId" class="col-sm-2 col-form-label  text-end " for="basic-default-name"></label>
                                                <div class="col-sm-4 ">
                                                    <input type="text" readonly="" class="form-control-plaintext" value="@Model.CommunityName">
                                                </div>
                                                <label asp-for="MotherTongueId" class="col-sm-2 col-form-label  text-end " for="basic-default-name"></label>
                                                <div class="col-sm-4 ">
                                                    <input type="text" readonly="" class="form-control-plaintext" value="@Model.MothorTongueName">
                                                </div>
                                            </div>
                                            @*----------------------*@
                                            <div class="mb-1 row">
                                                <label asp-for="MobilePhone" class="col-sm-2 col-form-label  text-end " for="basic-default-name"></label>
                                                <div class="col-sm-4 ">
                                                    <input type="text" readonly="" class="form-control-plaintext" value="@Model.MobilePhone">
                                                </div>
                                            </div>
                                            @*----------------------*@
                                            <div class="mb-1 row">
                                                <label asp-for="PortalEmail" class="col-sm-2 col-form-label  text-end " for="basic-default-name"></label>
                                                <div class="col-sm-4 ">
                                                    <input type="text" readonly="" class="form-control-plaintext" value="@Model.PortalEmail">
                                                </div>

                                                <label asp-for="AlternateEmail" class="col-sm-2 col-form-label  text-end " for="basic-default-name"></label>
                                                <div class="col-sm-4 ">

                                                    <input type="email" asp-for="AlternateEmail" class="form-control " placeholder="Alternate Email Address">
                                                    <span asp-validation-for="AlternateEmail" class="text-danger"></span>
                                                </div>

                                            </div>
                                            @*----------------------*@
                                            <div class="mb-1 row">
                                                <label asp-for="PermanentAddress1" class="col-sm-2 col-form-label  text-end " for="basic-default-name"></label>
                                                <div class="col-sm-4 ">
                                                    <input type="text" readonly="" class="form-control-plaintext" value="@Model.PermanentAddress1">
                                                </div>

                                                <label asp-for="PermanentAddress2" class="col-sm-2 col-form-label  text-end " for="basic-default-name"></label>
                                                <div class="col-sm-4 ">
                                                    <input type="text" readonly="" class="form-control-plaintext" value="@Model.PermanentAddress2">
                                                </div>
                                            </div>
                                            @*----------------------*@
                                            <div class="mb-1 row">
                                                <label asp-for="PCity" class="col-sm-2 col-form-label  text-end " for="basic-default-name"></label>
                                                <div class="col-sm-4 ">
                                                    <input type="text" readonly="" class="form-control-plaintext" value="@Model.PCity">
                                                </div>
                                                <label asp-for="PCountryId" class="col-sm-2 col-form-label  text-end " for="basic-default-name"></label>
                                                <div class="col-sm-4 ">
                                                    <input type="text" readonly="" class="form-control-plaintext" value="@Model.PCountryName">
                                                </div>

                                            </div>
                                            @*----------------------*@
                                            <div class="mb-1 row">
                                                <label asp-for="PStateId" class="col-sm-2 col-form-label  text-end " for="basic-default-name"></label>
                                                <div class="col-sm-4 ">
                                                    <input type="text" readonly="" class="form-control-plaintext" value="@Model.PStateName">
                                                </div>
                                                <label asp-for="PZip" class="col-sm-2 col-form-label  text-end " for="basic-default-name"></label>
                                                <div class="col-sm-4 ">
                                                    <input type="text" readonly="" class="form-control-plaintext" value="@Model.PZip">
                                                </div>

                                            </div>
                                            @*----------------------*@
                                        </div>
                                    </div>
                                </div>
                            </div>

                            @*====================================== OTHER INFORMATION Card==============================*@
                            <div class="card mb-4">
                                <div class="card-header d-flex align-items-center justify-content-between">
                                    <h5 class="mb-0">
                                        <span>
                                            OTHER INFORMATION
                                        </span>
                                    </h5>
                                </div>
                                <div class="card-body">
                                    <div class="row">
                                        <div class="col">
                                            @*----------------------*@
                                            <div class="mb-1 row">
                                                <label asp-for="InstanceClassificationId" class="col-sm-5 col-form-label  text-end " for="basic-default-name"></label>
                                                <div class="col-sm-4 ">
                                                    <input type="text" readonly="" class="form-control-plaintext" value="@Model.ClassificationName">
                                                </div>
                                            </div>
                                            <div class="mb-1 row">
                                                <label asp-for="InstanceSubClassificationId" class="col-sm-5 col-form-label  text-end " for="basic-default-name"></label>
                                                <div class="col-sm-4 ">
                                                    <input type="text" readonly="" class="form-control-plaintext" value="@Model.SubClassificationName">
                                                </div>
                                            </div>
                                            @*----------------------*@
                                            <div class="mb-1 row">
                                                <label asp-for="ClassTeacherId" class="col-sm-5 col-form-label  text-end " for="basic-default-name"></label>
                                                <div class="col-sm-4 ">
                                                    <input type="text" readonly="" class="form-control-plaintext" value="@Model.ClassTeacherName">
                                                </div>
                                            </div>

                                            @*----------------------*@


                                        </div>
                                    </div>
                                </div>
                            </div>

                            @*=================================================GENERAL INFORMATION Card========================*@
                            <div class="card mb-4">
                                <div class="card-header d-flex align-items-center justify-content-between">
                                    <h5 class="mb-0">
                                        <span>
                                            GENERAL INFORMATION
                                        </span>
                                    </h5>
                                </div>
                                <div class="card-body">
                                    <div class="row">
                                        <div class="col">
                                            @*----------------------*@
                                            <div class="row mb-1 ">
                                                <label asp-for="StudentIdentificationMarks" class="col-sm-5 col-form-label  text-end " for="basic-default-name"></label>
                                                <div class="col-sm-4 ">
                                                    <input type="text" readonly="" class="form-control-plaintext" value="@Model.StudentIdentificationMarks">
                                                </div>
                                            </div>
                                            @*----------------------*@
                                            <div class="row mb-1 ">
                                                <label asp-for="SiblingsInSameCollege" id="LblSiblingsInSameCollege" class="col-sm-5 col-form-label  text-end " for="basic-default-name"></label>
                                                <div class="col-sm-4 ">
                                                    @if (Model.SiblingsInSameCollege == "1")
                                                    {
                                                        Text = "Yes";
                                                    }
                                                    else
                                                    {
                                                        Text = "No";
                                                    }
                                                    <input type="text" readonly="" class="form-control-plaintext" value="@Text">
                                                </div>
                                            </div>
                                            @*----------------------*@
                                            @*<div class="row mb-1 ">
                                                    <label asp-for="SiblingsOtherCollege" id="LblSiblingsOtherCollege" class="col-sm-5 col-form-label  text-end " for="basic-default-name"></label>
                                                    <div class="col-sm-4 ">
                                                        @if (Model.SiblingsOtherCollege == "1")
                                                        {
                                                            Text = "Yes";
                                                        }
                                                        else
                                                        {
                                                            Text = "No";
                                                        }
                                                        <input type="text" readonly="" class="form-control-plaintext" value="@Text">
                                                    </div>
                                                </div>*@
                                            @*----------------------*@
                                            <div class="row mb-1 ">
                                                <label asp-for="PhysicallyChalenged" class="col-sm-5 col-form-label  text-end " for="basic-default-name"></label>
                                                <div class="col-sm-4 ">
                                                    <input type="text" readonly="" class="form-control-plaintext" value="@Model.PhysicallyChalenged">
                                                </div>
                                            </div>

                                            @*----------------------*@
                                            <div class="row mb-1 ">
                                                <label asp-for="PhysicallyChallengedDetails" class="col-sm-5 col-form-label  text-end " for="basic-default-name"></label>
                                                <div class="col-sm-4 ">
                                                    <input type="text" readonly="" class="form-control-plaintext" value="@Model.PhysicallyChallengedDetails">
                                                </div>
                                            </div>

                                            @*----------------------*@
                                        </div>
                                    </div>
                                </div>
                            </div>


                            @*============================================ LOGIN INFORMATION Card============================*@
                            <div class="card mb-4">
                                <div class="card-header d-flex align-items-center justify-content-between">
                                    <h5 class="mb-0">
                                        <span>
                                            LOGIN INFORMATION
                                        </span>
                                    </h5>
                                </div>
                                <div class="card-body">
                                    <div class="row">
                                        <div class="col">
                                            @*----------------------*@

                                            @*----------------------*@
                                            <div class="row mb-1">
                                                <label asp-for="UserName" class="col-sm-5 col-form-label  text-end " for="basic-default-name"></label>
                                                <div class="col-sm-4 ">
                                                    <input type="text" readonly="" class="form-control-plaintext" value="@Model.UserName">
                                                </div>
                                            </div> @*----------------------*@
                                            <div class="row mb-3 HideFileds" style="display:none !important">
                                                <label asp-for="Password" class="col-sm-4 col-form-label  text-end required" for="basic-default-name"></label>
                                                <div style="display:none"> <input type="hidden" value="@ViewBag?.identitypassword" id="IdentityPassword" readonly /></div>
                                                <div class="col-sm-4 ">
                                                    <input asp-for="Password" id="Password" class="form-control" placeholder="Password" required />
                                                    <span asp-validation-for="Password" class="text-danger "></span>
                                                </div>
                                            </div>
                                            @*----------------------*@
                                            <div class="row mb-3 HideFileds" style="display:none !important">
                                                <label asp-for="ConfirmPassword" class="col-sm-4 col-form-label  text-end required" for="basic-default-name"></label>
                                                <div class="col-sm-4 ">
                                                    <input asp-for="ConfirmPassword" id="ConPassword" class="form-control" placeholder="Confirm Password" required />
                                                    <span asp-validation-for="ConfirmPassword" class="text-danger "></span>
                                                </div>
                                            </div>
                                            @*----------------------*@
                                            <div class="row mb-3 HideFileds" style="display:none !important">
                                                <label asp-for="SecurityQuestionId" class="col-sm-4 col-form-label  text-end required" for="basic-default-name"></label>
                                                <div class="col-sm-4 ">
                                                    <div class="position-relative">
                                                        <select id='DdlPickUpStop' asp-for="SecurityQuestionId" class="select2 form-select select2-hidden-accessible" asp-items="@(new SelectList(ViewBag.DdlSecurityQuestionList, "Value", "Text"))" required>
                                                            <option value=''>---------Select---------</option>
                                                        </select>
                                                        <span asp-validation-for="SecurityQuestionId" class="text-danger"></span>
                                                    </div>
                                                </div>
                                            </div>
                                            @*----------------------*@
                                            <div class="row mb-3 HideFileds" style="display:none !important">
