                                            <select id='DdlLMSCategory' asp-for="LMSCategory" class="select2 form-select select2-hidden-accessible  ClsClear" onchange="CommonDropdownFunction('GET', '/Users/DdlLMSCategory_Calingfunction?Id=' + this.value, 'DdlLMSSubCategory', '------Select------', true)" asp-items="@(new SelectList(ViewBag.DdlLMSCategoryList, "Value", "Text"))" required>
                                                <option value=''>------Select------</option>
                                            </select>
                                            <span asp-validation-for="LMSCategory" class="text-danger"></span>
                                        </div>
                                    </div>
                                    <label asp-for="LMSSubCategory" class="col-sm-2 col-form-label  text-end required ClsDivTeachers" for="basic-default-name"></label>
                                    <div class="col-sm-4 ClsDivTeachers">
                                        <div class="position-relative">
                                            <select id='DdlLMSSubCategory' asp-for="LMSSubCategory" class="select2 form-select select2-hidden-accessible  ClsClear DdlCascadDropdowns" asp-items="@(new SelectList(ViewBag.DdlLMSSubCategoryIdList,"Value","Text"))" disabled required>
                                                <option value=''>------Select------</option>
                                            </select>
                                            <span asp-validation-for="LMSSubCategory" class="text-danger"></span>
                                        </div>
                                    </div>

                                </div>
                                @*----------------------*@
                            </div>
                        </div>
                    </div>

                </div>


