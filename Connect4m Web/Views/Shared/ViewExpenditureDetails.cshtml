                                                </div>
                                                <span asp-validation-for="VendorCategory" class="text-danger"></span>

                                            </div>

                                            <div class="form-group col-2 ">
                                                <label asp-for="RemindAt" class="control-label  space"></label>

                                            </div>
                                            <div class="form-group col-4">

                                                <input asp-for="RemindAt" class="form-control" id="reminddate_SEM" disabled />
                                                <span asp-validation-for="RemindAt" class="text-danger"></span>

                                            </div>

                                            <div class="form-group col-2 ">
                                                <label asp-for="DocName" class="control-label space"></label>

                                            </div>


                                            <div class="form-group col-4">

                                                @*<input asp-for="DocName" class="form-control" disabled type="file" id="DocmentName" accept=".doc, .docx, .pdf, .jpeg, .jpg, .png, .gif" />*@
                                                @*<input asp-for="DocName" class="form-control" disabled type="file" id="DocmentName" accept=".doc, .docx, .pdf, .jpeg, .jpg, .png, .gif" />*@

                                                @{ if (Model != null && Model.DocNameEdit != "")
                                                    {
                                                        <input asp-for="DocNameEdit" class="form-control" type="text" readonly id="DocNameEdit" />

                                                    } }
                                                <span asp-validation-for="DocName" class="text-danger"></span>

                                            </div>

                                            <div class="form-group col-2">
                                                <label asp-for="Description" class="control-label space"></label>

                                            </div>
                                            <div class="form-group col-4">
                                                <textarea asp-for="Description" class="form-control" disabled rows="10" style="height: 85px; border: 1px solid #00000021;"></textarea>


                                            </div>





                                        </div>



