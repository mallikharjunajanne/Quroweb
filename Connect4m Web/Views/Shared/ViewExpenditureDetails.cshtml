                                            </div>
                                            <div class="form-group col-4">

                                                <div id="Drop_Format_SEM_1">
                                                    @Html.DropDownListFor(
x => x.YearId,
 new SelectList(ViewBag.Years, "Value", "Text"),
 " ", new { @class = "form-control readonly", @id = "dropdown_Year_SEM", @style = "color:black;", @disabled = true })

                                                </div>
                                                <span asp-validation-for="YearId" class="text-danger"></span>

                                            </div>
                                            <div class="form-group col-2">


                                                <label asp-for="MonthId" class="control-label  space"></label>
