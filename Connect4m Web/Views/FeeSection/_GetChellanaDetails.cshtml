    // Calculate the total sum of amount values and update the target element
    function calculateTotalAmount_a() {
        var totalAmount_a = 0;

        var amountElements_a = document.querySelectorAll('.feetable2a'); // Select all b elements within .feetable2


        for (var i = 0; i < amountElements_a.length; i++) {
            var amount_a = parseFloat(amountElements_a[i].textContent.replace(/,/g, '')); // Remove commas and parse

            totalAmount_a += amount_a;

        }

        return totalAmount_a.toFixed(2); // Return the total amount formatted to two decimal places

    }
   



    // Function to convert a numeric amount to words
    function convertNumberToWords(number) {
        var units = ['', 'one', 'two', 'three', 'four', 'five', 'six', 'seven', 'eight', 'nine'];
        var teens = ['eleven', 'twelve', 'thirteen', 'fourteen', 'fifteen', 'sixteen', 'seventeen', 'eighteen', 'nineteen'];
        var tens = ['', 'ten', 'twenty', 'thirty', 'forty', 'fifty', 'sixty', 'seventy', 'eighty', 'ninety'];
        var thousands = ['', 'thousand', 'million', 'billion', 'trillion'];

        function convertLessThanOneThousand(number) {
            var words = '';

            if (number >= 100) {
                words += units[Math.floor(number / 100)] + ' hundred';
                number %= 100;
                if (number > 0) {
                    words += ' and ';
                }
            }

            if (number >= 11 && number <= 19) {
                words += teens[number - 11];
            } else if (number >= 20 || number === 10) {
                words += tens[Math.floor(number / 10)];
                number %= 10;
                if (number > 0) {
                    words += ' ';
                }
            }

            if (number > 0) {
                words += units[number];
            }

            return words;
        }

        if (number === 0) {
            return 'zero';
        }

        var wordGroups = [];
        var numString = number.toString();
        var numChunks = Math.ceil(numString.length / 3);

        for (var i = 0; i < numChunks; i++) {
            var chunk = parseInt(numString.slice(-3), 10);
            numString = numString.slice(0, -3);

            if (chunk !== 0) {
                var chunkWords = convertLessThanOneThousand(chunk);
                if (i > 0) {
                    chunkWords += ' ' + thousands[i];
                }
                wordGroups.unshift(chunkWords);
            }
        }

        return wordGroups.join(' ');
    }
