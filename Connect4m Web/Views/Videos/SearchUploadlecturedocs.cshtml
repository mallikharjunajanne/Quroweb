                        <form id="formsearchvideouploading">
                            <div>
                                <div class="row">
                                    <div class="col">

                                        @*=====================         DROP DOWN               =================*@

                                        <div class="row mb-3">

                                            <label class="col-sm-2 col-form-label text-end" for="basic-default-name"> Department</label>
                                            <div class="col-sm-4">
                                                <select name="Department" id="Department" class="select2 form-select select2-hidden-accessible" multiple asp-items="@ViewBag.Departmentlist">
                                                </select>
                                            </div>

                                            <label class="col-sm-2 col-form-label text-end" for="basic-default-name">Class</label>
                                            <div class="col-sm-4">
                                                <select name="ClassificationIds" id="ClassificationIds" multiple class="select2 form-select select2-hidden-accessible">
                                                </select>
                                            </div>
                                        </div>
                                        @*=====================         DROP DOWN               =================*@

                                        <div class="row mb-3">

                                            <label class="col-sm-2 col-form-label text-end" for="basic-default-name"> Subject</label>
                                            <div class="col-sm-4">
                                                <select name="InstanceSubClassificationId" id="InstanceSubClassificationId" multiple class="select2 form-select select2-hidden-accessible">
                                                </select>
                                            </div>

                                            <label class="col-sm-2 col-form-label text-end" for="basic-default-name">Subject Tool</label>
                                            <div class="col-sm-4">
                                                <select name="InstanceSubjectsId" id="InstanceSubjectsId" multiple class="select2 form-select select2-hidden-accessible">
                                                </select>
                                            </div>
                                        </div>

                                        @*=====================         BUTTONS                =================*@

                                        <div class="row justify-content">
                                            <div class="col-sm-5">
                                            </div>

                                            <div class="col-sm-7">
                                                <button type="reset" class="btn btn-warning waves-effect waves-light" id="videos_uploading_clear">Clear</button>
                                                <button type="submit" class="btn btn-success waves-effect waves-light" id="videos_uploading_search">Search</button>

                                            </div>
                                        </div>



                                    </div>
                                </div>
                            </div>

                        </form>
                        @*=============================  Form End ================================*@

                    </div>

                    <div class="card">

                        <h6 class="card-title mb-0">
                            <span>YOUR SEARCH RESULTED <span id="totalrecords_uploadlecturedocs" class="badge badge-center rounded-pill bg-primary bg-glow">0</span> RECORD(S).</span>
                        </h6>
                        <div class="table-responsive text-nowrap">
                            <table id="tbluploadlecturedocs" class="link table table-hover table-bordered">
                                <thead class="table-dark">
                                    <tr>
                                        <th hidden>

                                        </th>
                                        <th>
                                            <label> S.NO</label>
                                        </th>
                                        <th hidden>

                                        </th>

                                        <th>
                                            <label>Department</label>
                                        </th>
                                        <th>
                                            <label> Class </label>
                                        </th>
                                        <th>
                                            <label>Subject </label>
                                        </th>
                                        <th>
                                            <label> Subject Tool</label>
                                        </th>
                                        <th>
                                            <label>Restriction</label>
                                        </th>
                                        <th>
                                            <label>Lecture /Doc Name</label>

                                        </th>
                                        <th>
                                            <label> Lecture/Docs</label>

                                        </th>
                                        <th hidden>

                                        </th>
                                        <th hidden>

                                        </th>
                                        <th hidden>

                                        </th>
                                        <th hidden>

